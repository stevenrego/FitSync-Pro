{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "buildCommand": "npx expo export:web --output-dir dist",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/_expo/(.*)", "destination": "/_expo/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
