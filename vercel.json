{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "npx expo export --platform web --output-dir dist",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "dist"
}
